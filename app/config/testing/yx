CREATE TABLE `yx_address` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `user_id` int(11) DEFAULT '0',
  `shipper` varchar(50) DEFAULT '' COMMENT '托运人',
  `city_id` int(11) DEFAULT '0' COMMENT '城市',
  `address` char(200) DEFAULT '' COMMENT '地址',
  `phone` varchar(20) DEFAULT '' COMMENT '手机',
  `is_default` tinyint(1) DEFAULT '0' COMMENT '1,默认地址',
  `create_time` int(10) DEFAULT '0' COMMENT '创建时间',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=1 DEFAULT CHARSET=utf8;