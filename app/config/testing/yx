/*Table structure for table `yx_job` */

DROP TABLE IF EXISTS `yx_job`;

CREATE TABLE `yx_job` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `title` varchar(50) NOT NULL DEFAULT '' COMMENT '名称',
  `number` int(10) NOT NULL DEFAULT '1' COMMENT '招聘人数',
  `location` varchar(30) NOT NULL DEFAULT '' COMMENT '工作地点',
  `department` varchar(30) NOT NULL DEFAULT '' COMMENT '部门',
  `date` int(10) NOT NULL DEFAULT '0' COMMENT '结束时间',
  `description` text COMMENT '职位描述',
  `requirement` text COMMENT '职位要求',
  `create_time` int(10) NOT NULL DEFAULT '0' COMMENT '创建时间',
  `user_id` int(11) NOT NULL DEFAULT '0' COMMENT '创建人',
  `status` tinyint(1) NOT NULL DEFAULT '0' COMMENT '状态：1，发布显示',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=4 DEFAULT CHARSET=utf8;

